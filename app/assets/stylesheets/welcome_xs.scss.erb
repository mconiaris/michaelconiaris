/*====================
CSS Variables
====================*/

/*Screen Sizes*/
$screen-lg-min: 940px;
$screen-md-min: 720px;
$screen-sm-min: 576px;
$screen-xs-min: 350px;


/* Very Small Devices (iPhone 5, width less than 576px) */
@media (max-width: $screen-xs-min) {

	/*Jumbotron*/
	.jumbotron {
	  height: 11em;
	}

}